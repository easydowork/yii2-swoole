{
  "name": "dacheng-php/yii2-swoole",
  "description": "Yii2 Swoole extension, provides coroutine http server and more coroutine features.",
  "keywords": [
    "Yii2",
    "Swoole",
    "Coroutine",
    "HTTP Server",
    "Connection Pools"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-swoole": "^6.0",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev": {
    "swoole/ide-helper": "6.0.2",
    "yidas/yii2-composer-bower-skip": "2.0.13",
    "yiisoft/yii2-httpclient": "^2.0",
    "yiisoft/yii2-queue": "^2.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Dacheng\\Yii2\\Swoole\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "apcu-autoloader": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
