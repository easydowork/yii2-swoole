# Yii2-Swoole Environment Configuration

# Database Configuration
YII_DB_DSN=mysql:host=127.0.0.1;dbname=yii2basic
YII_DB_USERNAME=root
YII_DB_PASSWORD=

# Database Pool Configuration
YII_DB_POOL_MAX_ACTIVE=10
YII_DB_POOL_WAIT_TIMEOUT=5.0
YII_DB_SCHEMA_CACHE_DURATION=3600

# Redis Configuration
YII_REDIS_HOST=127.0.0.1
YII_REDIS_PORT=6379
YII_REDIS_DATABASE=0
YII_REDIS_PASSWORD=

# Redis Pool Configuration
YII_REDIS_POOL_MAX_ACTIVE=20
YII_REDIS_POOL_WAIT_TIMEOUT=5.0

# Session Configuration
YII_SESSION_TIMEOUT=1440

# Queue Configuration
YII_QUEUE_CONCURRENCY=10

# Log Configuration
YII_LOG_CHANNEL_SIZE=10000

