{
    "name": "dacheng-php/yii2-api",
    "description": "Yii 2 API Project with Swoole",
    "keywords": ["yii2", "framework", "api", "swoole", "coroutine"],
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-swoole": "^6.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-redis": "^2.0",
        "yiisoft/yii2-queue": "^2.0",
        "dacheng-php/yii2-swoole": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "./"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        },
        "process-timeout": 1800,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postInstall": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}

